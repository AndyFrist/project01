<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hh.gridview_recyclerview.activity.BrokenlineActivity">

    <com.hh.gridview_recyclerview.calendar.week.WeekCalendarView
        android:layout_width="match_parent"
        android:layout_height="48dp" />


    <com.hh.gridview_recyclerview.calendar.month.MonthCalendarView
        android:layout_width="match_parent"
        android:layout_height="288dp" />

    <com.hh.gridview_recyclerview.View.CalendarView
        android:id="@+id/calendarview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
